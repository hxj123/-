/* pages/2hand_trade/2hand_trade.wxss */
page{
  width: 100%;
  height: 100%;
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
.tab{
  width:100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.tab-header{
  width: 100%;
  height: 80rpx;
  background: rgb(245, 245, 245)b
}
.tab-nav{
  display: flex;
}
.tab-nav view{
  height: 85rpx;
  line-height: 85rpx;
  width: 33.33%;
  font-size: 32rpx;
  text-align: center;
  color: rgb(53, 53, 53);
  font-weight: bold;
}
.line{
  width:100%;
  background:black;
}
view.on{
  color: #cc492c;
  background: #fff;
  /* border-bottom: 2px solid rgb(197, 73, 73); */
}
.lon{
  color: #cc492c;
  background: #fff;
  border-bottom: 2px solid rgb(197, 73, 73);
}
.tab-box{
  width: 100%;
  flex:1;
}
scroll-view{
  height: 100%;
  width: 100%;
}

swiper-item{
  height:100%;
}
.picker_value{
  display: flex;
  align-items: center;
}
.picker_value .text2{
  /* color:#888; */
  font-size: 32rpx;
}
.book_item input{
  width: 420rpx;
  display: inline-block;
  font-size: 32rpx;
  padding-right: 10rpx;
  text-align:right;
}
.book_item{
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 32rpx;
}
.book_item image{
  width: 40rpx;
  height: 40rpx;
}
.book_item .text1{
  color: black;
  font-size: 32rpx;
}
.book_block{
  width: 100%;
  border-bottom: 1px solid rgb(219, 219, 219);
  padding: 12px 20px;
  box-sizing: border-box;
}
.book_block .weui-uploader__bd {
  padding: 1rpx;
}
.loadmore_toast{
  position: absolute;
  background: #000000;
  opacity: 0.7;
  color: white;
  border-radius: 18px;
  bottom: 30px;
  padding: 3px 10px;
  font-size: 15px;
  z-index: 100
}

.tip_block{
  width: 100%;
  height: 100%;
  display:flex;
  align-items: center;
  justify-content: center;
  color: #aaa
}

.a{
  display:flex;
  justify-content: center;
  align-items: center
}

.scroll_content{
  overflow:auto;
  margin:auto;
  position:absolute;
  top:0;
  bottom:0;
  left:0;
  right:0;
}
.ppw_container {
  position: fixed;
  bottom: 0;
  width: 80%;
  height: 100%;
  transform: translateX(170%);
  transition: all 0.3s ease;
  z-index: 12;
  background: #fff;
}
.ppw-show .ppw_container {
  transform: translateX(26%);
}
.ppw-show .ppw{
  display: block;
}
.ppw_container-bottom {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 30%;
  transform: translateY(100%);
  transition: all 0.3s ease;
  z-index: 12;
  background: #fff;
}
.ppw-show-bottom .ppw_container-bottom {
  transform: translateY(0);
}
.ppw-show-bottom .ppw-bottom{
  display: block;
}
checkbox .colleges {
  transform: scale(0.5);
}
.myMask {
  height: 100%;
  width: 100%;
  background: #000;
  z-index: 3;
  top:0;
  left:0;
  opacity:0.5;
  position: fixed;
}
.money{
  display: flex;
  align-items: center;
}
.money input{
  color: rgb(214, 0, 0);
  width: 100%;
  height: 100%;
}
.button1{
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  width:88%;
}
.centerColumn{
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}
.centerRow{
  display: flex;
  align-items: center;
  position: relative;
}
.filter {
  height: 30px;
  width: 30px;
  padding: 0 5rpx;
}
.book_infos {
  width: 100%;
  height: 100px;
  border-bottom: 1px solid rgb(219, 219, 219);
  padding: 0 15px 0 15px;
  box-sizing: border-box;
  align-items: center;
  display: flex;
  justify-content: space-between;
}
.left_info {
  margin: 10px 5px;
  width: 80%;
}
.left_info view{
  font-size: 16px;
}
.weui-uploader__img {
  padding-top: 10px;
}
.position {
  display: flex;
}
.searchbar-result{
  margin-top: 0;
  font-size: 14px;
}
.searchbar-result:before{
  display: none;
}
.weui-cell{
  padding: 12px 15px 12px 35px;
  z-index: 20;
}
.weui-loadmore {
  height: 50px;
}
.loadmore-top {
  height: 50px;
  transform: translateY(-100%);
  transition: all 0.3s ease;
  z-index: -1;

  display: flex;
  align-items: center;
  position: relative;
}
.weui-loadmore-show {
  transform: translateY(0);
}


