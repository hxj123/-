page{
  width: 100%;
  height: 100%;
}

.container{
  background: #eee
}

.block{
  width: 100%;
  height: 120rpx;
  padding: 10dp;
  display: flex;
  align-items: center;
  background: white;
  border-bottom: 1px solid #eee;
  position: relative;
}

.block text{
  width: 140rpx;
  font-size: 32rpx;
  margin-left: 60rpx;
  margin-right: 30rpx;
  font-weight: bolder;
}

.block input{
  width: 260rpx;
  font-size: 32rpx;
  padding: 5rpx;
}

#feeAccount{
  width: 500rpx;
}

.button{
  width: 100%;
  position: relative;
  margin-top: 10rpx;
  display: flex;
  justify-content: center
}

.button button{
  width: 95%;
  height: 100rpx;
  background: none;
}

.button button::after{
  border: none;
}

.button image{
  width: 95%;
  height: 100rpx;
  position: absolute;
}

form{
  width: 100%;
}
